# reference ······················
nf16 		()	{ echo;for i in {0..15};do printf "\e[38;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 0 ];then echo;fi;done;echo;}
nf216		()	{ echo;for i in {16..231};do printf "\e[38;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 4 ];then echo;fi;done;}
nf24 		()	{ echo;for i in {232..255};do printf "\e[38;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 4 ];then echo;fi;done;}
nb16 		()	{ echo;for i in {0..15};do printf "\e[48;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 0 ];then echo;fi;done;echo;}
nb216		()	{ echo;for i in {16..231};do printf "\e[48;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 4 ];then echo;fi;done;}
nb24 		()	{ echo;for i in {232..255};do printf "\e[48;5;%sm %4s \e[0m" "${i}" "${i}";if [ $((($i + 1) % 6)) == 4 ];then echo;fi;done;}
# generate foreground ············
nf256		()	{ nf16 && nf216 && nf24 ; }
# generate background ············
nb256		()	{ nb16 && nb216 && nb24 ; }
# generate both ··················
nfb256	()	{ nf256 && nb256 ; }
